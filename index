	// Nav bar scroll animation
	const navBar = document.getElementById('navBar');

	const animationObserver = new IntersectionObserver((entries, observer) => {
		for (const entry of entries) {
			entry.target.classList.toggle('nav--compress', !entry.isIntersecting)
		}
	});

	animationObserver.observe(navBar);
	
	// Mobile menu hamburger toggle
	const navToggleBtn = document.getElementById('navToggleBtn');
	const body = document.body;
	const html = document.documentElement;
	
	navToggleBtn.addEventListener('mousedown', function(e){
		navToggleBtn.classList.toggle('nav__toggle--active');
		body.classList.toggle('menu-open');
		html.classList.toggle('menu-open');
	});
</script></div></nav><section class="hero-section"><div data-delay="4000" data-animation="slide" class="slider-6 w-slider" data-autoplay="true" data-easing="ease" data-hide-arrows="false" data-disable-swipe="false" data-autoplay-limit="0" data-nav-spacing="3" data-duration="500" data-infinite="true"><div class="mask-6 w-slider-mask"><div class="slide-6 w-slide"><div class="hero-plax"><div class="kd-header-menu-background-image-wrapper"><div data-poster-url="https://cdn.prod.website-files.com/6777ca72cf24680980ebd1e0%2F677c913724b38a6a210eb313_8626668-hd_1920_1080_25fps-poster-00001.jpg" data-video-urls="https://cdn.prod.website-files.com/68d17b52669acb852e6230b0/68d17b55669acb852e623193_8626668-hd_1920_1080_25fps-transcode.mp4,https://cdn.prod.website-files.com/68d17b52669acb852e6230b0/68d17b55669acb852e623193_8626668-hd_1920_1080_25fps-transcode.webm" data-autoplay="true" data-loop="true" data-wf-ignore="true" class="kd-header-menu-background-video w-background-video w-background-video-atom"><video id="609ef88e-e8c0-02ac-14cb-f69668c1c712-video" autoplay="" loop="" style="background-image:url(&quot;https://cdn.prod.website-files.com/6777ca72cf24680980ebd1e0%2F677c913724b38a6a210eb313_8626668-hd_1920_1080_25fps-poster-00001.jpg&quot;)" muted="" playsinline="" data-wf-ignore="true" data-object-fit="cover"><source src="https://cdn.prod.website-files.com/68d17b52669acb852e6230b0/68d17b55669acb852e623193_8626668-hd_1920_1080_25fps-transcode.mp4" data-wf-ignore="true"/><source src="https://cdn.prod.website-files.com/68d17b52669acb852e6230b0/68d17b55669acb852e623193_8626668-hd_1920_1080_25fps-transcode.webm" data-wf-ignore="true"/></video></div></div><div class="hero-plax__content"><header class="hero-plax__heading"><div class="banner-heading-wrapper"><div class="banner-heading">Experience the Taste of <span class="text-span">Tradition</span></div></div></header></div></div></div><div data-w-id="609ef88e-e8c0-02ac-14cb-f69668c1c723" class="slide-7 w-slide"><div class="her